{
  "name": "autoartch-mern-stack",
  "version": "1.0.0",
  "description": "AUTOARTCH - AI-Powered Smart Architecture Designer (MERN Stack)",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "npm run dev --prefix server",
    "client": "npm run dev --prefix client",
    "build": "npm run build --prefix client",
    "start": "npm start --prefix server",
    "install-all": "npm install; npm install --prefix server; npm install --prefix client",
    "setup": "npm run install-all && echo 'Setup complete! Run npm run dev to start the application'",
    "test": "npm test --prefix server",
    "lint": "npm run lint --prefix client"
  },
  "keywords": [
    "mern",
    "react",
    "nodejs",
    "express",
    "mongodb",
    "architecture",
    "design",
    "ai",
    "floor-plan",
    "3d-visualization"
  ],
  "author": "ArchSense Team",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^24.3.1",
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/archsense-mern.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/archsense-mern/issues"
  },
  "homepage": "https://github.com/your-username/archsense-mern#readme",
  "dependencies": {
    "drizzle-kit": "^0.31.5"
  }
}
